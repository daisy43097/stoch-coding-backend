{
   "name": "lulu-backend-mysql",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/node": "^16.11.10",
      "ts-node": "10.7.0",
      "typescript": "^4.6.4"
   },
   "dependencies": {
      "body-parser": "^1.19.1",
      "class-validator": "^0.14.0",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.17.2",
      "mongodb-memory-server": "^9.1.6",
      "mongoose": "^8.1.1",
      "nodemon": "^2.0.22",
      "winston": "^3.11.0"
   },
   "scripts": {
      "start": "nodemon src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "[__for TypeORM migration": "",
      "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
      "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync -d src/data-source.ts",
      "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
      "]__for TypeORM migration": "",
      "[__for TypeORM seeding": "",
      "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
      "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop -d src/data-source.ts",
      "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
      "__for TypeORM seeding]": ""
   }
}
